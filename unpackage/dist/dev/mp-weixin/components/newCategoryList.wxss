.boxStyle {
  padding: 20rpx;
}
.box_name {
  font-size: 36rpx;
  padding: 20rpx 0;
  margin-bottom: 20rpx;
  text-align: center;
  font-weight: 600;
}
.item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.item1 {
  width: 350rpx;
  margin-bottom: 10rpx;
  background: #FFFFFF;
}
.img {
  width: 350rpx;
  height: 340rpx;
}
.item1_name {
  width: 340rpx;
  text-align: center;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin: 10rpx 0;
}
.retail_price {
  width: 350rpx;
  text-align: center;
  font-size: 30rpx;
  color: #d81e06;
}
.more {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 36rpx;
  width: 350rpx;
  height: 450rpx;
  background: #FFFFFF;
}
.more_img {
  width: 40rpx;
  height: 40rpx;
  margin-left: 30rpx;
}
.active {
  margin-right: 10rpx;
}

